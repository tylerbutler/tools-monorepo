{
	"name": "test-cli",
	"version": "0.0.0",
	"private": true,
	"description": "A new CLI generated with oclif",
	"keywords": ["oclif"],
	"homepage": "https://github.com/packages/test-cli",
	"bugs": "https://github.com/packages/test-cli/issues",
	"repository": "packages/test-cli",
	"license": "MIT",
	"author": "Tyler Butler",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"test-cli": "./bin/run.js"
	},
	"files": ["/bin", "/dist", "/oclif.manifest.json"],
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts",
		"prepack": "oclif manifest && oclif readme",
		"postpack": "shx rm -f oclif.manifest.json",
		"test": "npm run test:vitest",
		"posttest": "pnpm run lint",
		"test:coverage": "vitest run test --coverage",
		"test:mocha": "mocha --forbid-only \"test/**/*.test.ts\"",
		"test:snapshots": "npm run update:test-snapshots",
		"test:vitest": "vitest run test",
		"update:test-snapshots": "vitest run test -u",
		"version": "oclif readme && git add README.md"
	},
	"oclif": {
		"bin": "test-cli",
		"commands": "./dist/commands",
		"dirname": "test-cli",
		"plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		}
	},
	"dependencies": {
		"@oclif/core": "^4.0.4",
		"@oclif/plugin-help": "^6.2.1",
		"@oclif/plugin-plugins": "^5.2.4"
	},
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "^4.0.3",
		"@types/chai": "^4.3.16",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.12.7",
		"chai": "^4.4.1",
		"eslint": "^8",
		"eslint-config-oclif": "^5",
		"eslint-config-oclif-typescript": "^3",
		"eslint-config-prettier": "^9",
		"mocha": "^10.4.0",
		"oclif": "^4.13.5",
		"shx": "^0.3.3",
		"ts-node": "^10.9.2",
		"typescript": "~5.5.4",
		"vitest": "2.0.5"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
