{
	"name": "@tylerbu/fluid-svelte",
	"version": "0.0.1",
	"private": true,
	"description": "Useful tools for building Svelte apps using the Fluid Framework.",
	"keywords": ["fluidframework", "svelte"],
	"bugs": "https://github.com/tylerbutler/tools-monorepo/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tylerbutler/tools-monorepo.git",
		"directory": "packages/fluid-svelte"
	},
	"license": "MIT",
	"author": "Tyler Butler <tyler@tylerbutler.com>",
	"sideEffects": ["**/*.css"],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"/CHANGELOG.md",
		"/THIRD-PARTY-LICENSES.txt"
	],
	"scripts": {
		"api:disabled": "api-extractor run --local",
		"build": "fluid-build . --task build",
		"check": "concurrently npm:check:format npm:check:svelte",
		"check:format": "biome check . --linter-enabled=false",
		"check:snapshot": "./bin/dev.js snapshot:compare --filepath test/commands/_snapshots/commands.json",
		"check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "concurrently npm:clean:build",
		"clean:build": "rimraf esm *.tsbuildinfo *.done.build.log",
		"compile": "vite build && npm run prepack",
		"dev": "vite dev",
		"format": "sort-package-json && biome check . --linter-enabled=false --write",
		"full": "fluid-build . --task full",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"release:license-file": "generate-license-file -c ../../.generatelicensefile.cjs",
		"test:coverage:disabled": "vitest run test --coverage",
		"test:disabled": "npm run test:vitest",
		"test:snapshots": "npm run update:test-snapshots",
		"test:vitest": "vitest run test",
		"update:test-snapshots": "vitest run test -u"
	},
	"dependencies": {
		"@fluidframework/presence": "^2.33.2",
		"fluid-framework": "^2.33.2",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@microsoft/api-extractor": "^7.52.7",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^20.12.7",
		"@vitest/coverage-v8": "^3.1.2",
		"generate-license-file": "^4.0.0",
		"publint": "^0.3.2",
		"rimraf": "^6.0.1",
		"sort-package-json": "*",
		"start-server-and-test": "^2.0.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tsx": "^4.19.4",
		"type-fest": "^4.40.1",
		"typescript": "~5.5.4",
		"vite": "^6.0.0",
		"vitest": "^3.1.2"
	}
}
