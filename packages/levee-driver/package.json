{
	"name": "@tylerbu/levee-driver",
	"version": "0.0.1",
	"description": "Driver for levee, a Fluid Framework service.",
	"bugs": "https://github.com/tylerbutler/tools-monorepo/issues",
	"license": "MIT",
	"author": "Tyler Butler <tyler@tylerbutler.com>",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.ts",
				"default": "./esm/index.js"
			}
		}
	},
	"main": "esm/index.js",
	"types": "esm/index.d.ts",
	"scripts": {
		"api": "api-extractor run --local",
		"build": "fluid-build . --task build",
		"build:test": "tsc --project ./test/tsconfig.json",
		"check:are-the-types-wrong": "attw --pack .",
		"check:biome": "biome check .",
		"check:format": "npm run check:biome",
		"clean": "rimraf --glob dist lib \"**/*.tsbuildinfo\" \"**/*.build.log\" _api-extractor-temp nyc",
		"compile": "tsc --project ./tsconfig.json",
		"format": "biome check . --linter-enabled=false --write",
		"full": "fluid-build . --task full",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"test": "npm run test:mocha",
		"test:mocha": "mocha --recursive \"lib/test/**/*.spec.*js\""
	},
	"dependencies": {
		"@fluidframework/core-interfaces": "^2.30.0",
		"@fluidframework/driver-definitions": "^2.30.0",
		"@fluidframework/driver-utils": "^2.30.0",
		"@fluidframework/routerlicious-driver": "^2.30.0",
		"jsrsasign": "^11.0.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.1",
		"@biomejs/biome": "~1.9.3",
		"@fluid-internal/mocha-test-setup": "^2.30.0",
		"@fluid-tools/build-cli": "^0.54.0",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/build-tools": "^0.54.0",
		"@fluidframework/eslint-config-fluid": "^5.7.3",
		"@microsoft/api-extractor": "7.47.8",
		"@types/jsrsasign": "^10.5.12",
		"@types/mocha": "^10.0.10",
		"@types/node": "^18.19.0",
		"@types/uuid": "^9.0.2",
		"concurrently": "^8.2.1",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"eslint": "~8.55.0",
		"mocha": "^10.8.2",
		"rimraf": "^4.4.0",
		"typescript": "~5.4.5"
	},
	"typeValidation": {
		"broken": {},
		"entrypoint": "internal"
	}
}
