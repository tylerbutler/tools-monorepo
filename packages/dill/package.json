{
	"name": "dill",
	"version": "0.0.1",
	"private": true,
	"description": "Simple file downloads with an API and CLI.",
	"keywords": ["download", "ci"],
	"homepage": "https://github.com/tylerbutler/tools-monorepo/packages/dill/",
	"bugs": "https://github.com/tylerbutler/tools-monorepo/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/tylerbutler/tools-monorepo.git",
		"directory": "packages/dill"
	},
	"license": "MIT",
	"author": "Tyler Butler",
	"type": "module",
	"exports": {
		".": {
			"types": "./esm/index.d.ts",
			"import": "./esm/index.js"
		},
		"./api": {
			"types": "./esm/api.d.ts",
			"import": "./esm/api.js"
		}
	},
	"types": "dist/index.d.ts",
	"bin": {
		"dill": "./bin/run.js"
	},
	"files": ["/bin", "/esm", "/oclif.manifest.json"],
	"scripts": {
		"build": "fluid-build . --task build",
		"check": "concurrently npm:check:format npm:check:snapshot",
		"check:format": "biome check . --linter-enabled=false",
		"check:snapshot": "./bin/dev.js snapshot:compare --filepath test/commands/_snapshots/commands.json",
		"clean": "concurrently npm:clean:build npm:clean:manifest",
		"clean:build": "rimraf esm *.tsbuildinfo *.done.build.log",
		"clean:manifest": "rimraf oclif.manifest.json",
		"command-snapshots": "./bin/dev.js snapshot:generate --filepath test/commands/_snapshots/commands.json",
		"compile": "tsc --project ./tsconfig.json",
		"format": "sort-package-json && biome check . --linter-enabled=false --apply",
		"full": "fluid-build . --task full",
		"generate": "concurrently npm:command-snapshots",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --apply",
		"manifest": "oclif manifest",
		"postpack": "npm run clean:manifest",
		"readme": "oclif readme --no-aliases",
		"sort-package-json": "sort-package-json",
		"test": "start-server-and-test start http://localhost:8080 test:vitest",
		"test:mocha": "mocha --forbid-only \"test/mocha/**/*.test.ts\"",
		"test:snapshots": "start-server-and-test start http://localhost:8080 update:test-snapshots",
		"test:vitest": "vitest run \"test/vitest\"",
		"update:test-snapshots": "vitest run \"test/vitest\" -u"
	},
	"oclif": {
		"bin": "dill-cli",
		"commands": {
			"strategy": "pattern",
			"target": "./dist/commands"
		},
		"devPlugins": ["@oclif/plugin-command-snapshot"],
		"dirname": "dill",
		"plugins": ["@oclif/plugin-help"]
	},
	"dependencies": {
		"@andrewbranch/untar.js": "^1.0.3",
		"@oclif/core": "^3.26.6",
		"@oclif/plugin-help": "^6.0.21",
		"@tinyhttp/content-disposition": "^2.2.0",
		"@tylerbu/cli-api": "workspace:^",
		"fflate": "^0.8.2",
		"file-type": "^19.0.0",
		"fs-extra": "^11.2.0",
		"mime": "^4.0.3",
		"node-fetch-native": "^1.6.4",
		"whatwg-mimetype": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@microsoft/api-extractor": "^7.43.4",
		"@oclif/plugin-command-snapshot": "^5.1.8",
		"@oclif/test": "^3.2.13",
		"@types/chai": "^4.3.14",
		"@types/debug": "^4.1.12",
		"@types/fs-extra": "^11.0.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.12.7",
		"@types/semver": "^7.5.8",
		"@types/tmp": "^0.2.6",
		"@types/whatwg-mimetype": "^3.0.2",
		"chai": "^4.4.1",
		"lefthook": "^1.6.10",
		"mocha": "^10.4.0",
		"oclif": "^4.10.7",
		"rimraf": "^5.0.5",
		"serve": "^14.2.3",
		"sort-package-json": "2.10.0",
		"start-server-and-test": "^2.0.3",
		"tmp-promise": "^3.0.3",
		"ts-node": "^10.9.2",
		"type-fest": "^4.17.0",
		"typescript": "~5.4.5",
		"vitest": "^1.6.0"
	},
	"packageManager": "pnpm@8.6.1+sha1.435b3c83c7de5c455ad80fd63ee7ecf3b67b949b",
	"engines": {
		"node": ">=18.0.0"
	},
	"pnpm": {
		"peerDependencyRules": {},
		"updateConfig": {
			"ignoreDependencies": ["@types/node"]
		}
	}
}
