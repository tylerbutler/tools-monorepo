{
	"name": "@tylerbu/sail",
	"version": "0.0.1",
	"description": "",
	"keywords": [],
	"bugs": "https://github.com/tylerbutler/sail-monorepo/issues",
	"license": "MIT",
	"author": "Tyler Butler <tyler@tylerbutler.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./esm/index.d.ts",
			"import": "./esm/index.js"
		}
	},
	"types": "esm/index.d.ts",
	"bin": {
		"sail": "./bin/run.js"
	},
	"files": [
		"/bin",
		"/CHANGELOG.md",
		"/esm",
		"/oclif.manifest.json",
		"/THIRD-PARTY-LICENSES.txt"
	],
	"scripts": {
		"build": "",
		"build:compile": "tsc --project ./tsconfig.json",
		"build:manifest": "oclif manifest",
		"build:readme": "oclif readme --no-aliases",
		"check": "concurrently npm:check:format",
		"check:format": "biome check . --linter-enabled=false",
		"clean": "concurrently npm:clean:build npm:clean:manifest",
		"clean:build": "rimraf --glob esm \"*.tsbuildinfo\" \"*.done.build.log\"",
		"clean:manifest": "rimraf oclif.manifest.json",
		"format": "biome check . --linter-enabled=false --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"postpack": "npm run clean:manifest",
		"sort-package-json": "sort-package-json",
		"start": "serve ./test/data --listen 8080",
		"test": "vitest run test",
		"test:coverage": "vitest run test --coverage",
		"test:snapshots": "npm run update:test-snapshots",
		"update:test-snapshots": "vitest run test -u"
	},
	"oclif": {
		"additionalHelpFlags": ["-h"],
		"additionalVersionFlags": ["-V"],
		"bin": "sail",
		"commands": "./esm/commands/",
		"devPlugins": [],
		"dirname": "sail",
		"flexibleTaxonomy": true,
		"helpOptions": {
			"hideAliasesFromRoot": true,
			"hideCommandSummaryInDescription": true,
			"showFlagNameInTitle": false,
			"showFlagOptionsInTitle": true,
			"stripAnsi": true
		},
		"plugins": [],
		"repositoryPrefix": "<%- repo %>/blob/main/packages/sail/<%- commandPath %>"
	},
	"dependencies": {
		"@manypkg/get-packages": "^2.2.2",
		"@oclif/core": "^4.2.3",
		"@oclif/plugin-help": "^6.2.21",
		"@tylerbu/sail-infrastructure": "workspace:^",
		"@tylerbu/cli-api": "^0.6.1",
		"async": "^3.2.6",
		"cosmiconfig": "^8.3.6",
		"date-fns": "^2.30.0",
		"debug": "^4.3.7",
		"detect-indent": "^6.1.0",
		"find-up": "^7.0.0",
		"fs-extra": "^11.2.0",
		"glob": "^7.2.3",
		"globby": "^11.1.0",
		"ignore": "^7.0.3",
		"json5": "^2.2.3",
		"lodash": "^4.17.21",
		"lodash.isequal": "^4.5.0",
		"multimatch": "^5.0.0",
		"picocolors": "^1.1.1",
		"picomatch": "^2.3.1",
		"picospinner": "^2.0.0",
		"rimraf": "^6.0.1",
		"semver": "^7.6.3",
		"simple-git": "^3.27.0",
		"sort-package-json": "2.14.0",
		"ts-deepmerge": "^7.0.1",
		"ts-morph": "^22.0.0",
		"type-fest": "^4.33.0",
		"typescript": "~5.5.4",
		"uncrypto": "^0.1.3",
		"yaml": "^2.6.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@microsoft/api-extractor": "^7.49.1",
		"@oclif/plugin-command-snapshot": "^5.2.26",
		"@oclif/test": "^4.1.7",
		"@types/async": "^3.2.24",
		"@types/chai": "^4.3.20",
		"@types/debug": "^4.1.12",
		"@types/fs-extra": "^11.0.4",
		"@types/glob": "^8.1.0",
		"@types/jsonfile": "^6.1.4",
		"@types/lodash.isequal": "^4.5.8",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20.12.7",
		"@types/picomatch": "^3.0.2",
		"@types/semver": "^7.5.8",
		"@vitest/coverage-v8": "^3.0.2",
		"c8": "^10.1.3",
		"oclif": "^4.17.13",
		"pathe": "^2.0.2",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"type-fest": "^4.33.0",
		"typescript": "~5.5.4",
		"vitest": "^3.0.2"
	},
	"packageManager": "pnpm@9.11.0",
	"engines": {
		"node": ">=18.0.0"
	},
	"nx": {
		"targets": {
			"build": {
				"executor": "nx:noop"
			},
			"test": {
				"executor": "nx:noop"
			},
			"check": {
				"executor": "nx:noop"
			},
			"ci": {}
		}
	}
}
