{
  "name": "lib-a",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "build:gen": "node -e \"require('fs').mkdirSync('dist/gen',{recursive:true});require('fs').writeFileSync('dist/gen/index.js','export const generated = true;\\n')\"",
    "build:test:esm": "tsc --project src/test/tsconfig.json",
    "build:test:cjs": "tsc --project src/test/tsconfig.cjs.json",
    "build:test:no-exact": "tsc --project src/test/tsconfig.no-exactOptionalPropertyTypes.json"
  },
  "devDependencies": {
    "typescript": "~5.5.4"
  }
}
