{
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"incremental": true,
		"inlineSources": true,
		"jsx": "react",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"noImplicitAny": false,
		// This is enforced in lint instead.
		"noUnusedLocals": false,
		"pretty": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"types": [],
	},
}
