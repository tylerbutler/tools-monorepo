{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "cli-api: pnpm test",
			"request": "launch",
			"runtimeArgs": ["run-script", "test"],
			"cwd": "${workspaceFolder}/packages/cli-api",
			"runtimeExecutable": "pnpm",
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
		},
		{
			"name": "dill: pnpm test",
			"request": "launch",
			"runtimeArgs": ["run-script", "test"],
			"cwd": "${workspaceFolder}/packages/dill-cli",
			"runtimeExecutable": "pnpm",
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
			"env": {
				"NODE_ENV": "test",
				"FORCE_NODE_FETCH": "true",
			},
		},
		{
			"name": "sort-tsconfig: pnpm test",
			"request": "launch",
			"runtimeArgs": ["run-script", "test"],
			"cwd": "${workspaceFolder}/packages/sort-tsconfig",
			"runtimeExecutable": "pnpm",
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
			"env": {
				"NODE_ENV": "test",
				"FORCE_NODE_FETCH": "true",
			},
		},
		{
			"name": "sort-tsconfig",
			"request": "launch",
			"program": "${workspaceFolder}/packages/sort-tsconfig/bin/run.js",
			"args": ["tsconfig.json", "-v"],
			"cwd": "${workspaceFolder}/packages/sort-tsconfig",
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
		},
		{
			"name": "tbu",
			"request": "launch",
			"program": "${workspaceFolder}/packages/cli/bin/dev.js",
			"args": [],
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
		},
		{
			"name": "repopo",
			"request": "launch",
			"program": "${workspaceFolder}/packages/repopo/bin/dev.js",
			"args": ["check", "-v"],
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
		},
		{
			"name": "repopo fix",
			"request": "launch",
			"program": "${workspaceFolder}/packages/repopo/bin/dev.js",
			"args": ["check", "-v", "--fix"],
			"skipFiles": ["<node_internals>/**"],
			"type": "node",
		},
		{
			"name": "dill: astro dev",
			"command": "./node_modules/.bin/astro dev",
			"cwd": "${workspaceFolder}/packages/dill-docs",
			"request": "launch",
			"type": "node-terminal",
		},

		{
			"type": "node",
			"request": "launch",
			"name": "Execute Command",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/bin/dev",
			"args": ["hello", "world"],
		},
	],
}
